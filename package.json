{
  "name": "firecrawl-simple-client",
  "version": "1.0.1",
  "description": "A TypeScript API client library for Firecrawl",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "openapi-ts": "openapi-ts",
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "build:clean": "npm run clean && npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\" \"!src/client/**\"",
    "docs": "echo \"Documentation generation not configured yet\"",
    "release": "npm run lint && npm run typecheck && npm run test && npm version",
    "prepublishOnly": "npm run build:clean",
    "publish": "npm run release && git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sacode/firecrawl-simple-client.git"
  },
  "homepage": "https://github.com/Sacode/firecrawl-simple-client#readme",
  "bugs": {
    "url": "https://github.com/Sacode/firecrawl-simple-client/issues"
  },
  "keywords": [
    "api",
    "client",
    "typescript",
    "http",
    "fetch",
    "firecrawl",
    "web-scraping"
  ],
  "author": "Dmitriy Safonov",
  "license": "MIT",
  "dependencies": {
    "@hey-api/client-axios": "^0.7.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@hey-api/openapi-ts": "^0.66.3",
    "@types/node": "^22.14.0",
    "typescript-eslint": "^8.29.0",
    "@vitest/coverage-v8": "^3.1.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}